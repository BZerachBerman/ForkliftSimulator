<?php
namespace DBAccessComponents;
// DBAccessInfo.php
class DBInfo {
    // Properties
    public $host;
    public $DBName;
    public $DBUser;
    public $DBPassword;

    // Constructor
    function __construct() {
        $this->host = 'localhost';
        $this->DBName = 'forklifts';
        $this->DBUser = 'root';
        $this->DBPassword = '';
    }
}